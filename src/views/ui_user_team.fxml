<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TeamUserController">
   <center>
      <AnchorPane minHeight="400.0" minWidth="600.0" styleClass="container" stylesheets="@../values/style.css">
         <children>
            <TableView fx:id="tvTeam" layoutX="20.0" layoutY="20.0" prefHeight="500.0" prefWidth="900.0" stylesheets="@../values/style.css">
               <columns>
                  <TableColumn fx:id="colStadium" prefWidth="178.0" text="Стадион" />
                  <TableColumn fx:id="colTrainer" prefWidth="130.0" text="Тренер" />
                  <TableColumn fx:id="colCity" prefWidth="168.0" text="Город" />
                  <TableColumn fx:id="colName" prefWidth="145.0" text="Название" />
                  <TableColumn fx:id="colLastRating" prefWidth="159.0" text="Место" />
               </columns>
            </TableView>
         </children>

         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </AnchorPane>
   </center>
</BorderPane>
