<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="437.0" prefWidth="713.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CityController">
   <children>
      <VBox layoutX="21.0" layoutY="27.0" prefHeight="200.0" prefWidth="56.0" spacing="15.0">
         <children>
            <Label text="id">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label text="name">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="93.0" layoutY="27.0" prefHeight="192.0" prefWidth="196.0" spacing="10.0">
         <children>
            <TextField fx:id="tfId" />
            <TextField fx:id="tfTitle" />
         </children>
      </VBox>
      <TableView fx:id="tvCity" layoutX="294.0" layoutY="27.0" prefHeight="347.0" prefWidth="405.0">
         <columns>
            <TableColumn fx:id="colId" prefWidth="55.0" text="id" />
            <TableColumn fx:id="colName" prefWidth="78.0" text="name" />
         </columns>
      </TableView>
      <Button fx:id="btnInsert" layoutX="21.0" layoutY="219.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="80.0" text="insert" />
      <Button fx:id="btnUpdate" layoutX="117.0" layoutY="219.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="80.0" text="update" />
      <Button fx:id="btnDelete" layoutX="209.0" layoutY="219.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="80.0" text="delete" />
   </children>
</AnchorPane>
