<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="437.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StadiumController">
   <children>
      <VBox layoutX="21.0" layoutY="27.0" prefHeight="200.0" prefWidth="123.0" spacing="15.0">
         <children>
            <Label text="Город">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label text="Название">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label prefHeight="20.0" prefWidth="89.0" text="Вметимость">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label prefHeight="20.0" prefWidth="89.0" text="Цена билета">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <VBox layoutX="159.0" layoutY="27.0" prefHeight="188.0" prefWidth="166.0" spacing="10.0">
         <children>
            <TextField fx:id="tfCity" editable="false" />
            <TextField fx:id="tfStadiumName" />
            <TextField fx:id="tfCapacity" />
            <TextField fx:id="tfTicketPrice" />
         </children>
      </VBox>
      <TableView fx:id="tvStadium" layoutX="576.0" layoutY="27.0" prefHeight="347.0" prefWidth="396.0">
         <columns>
            <TableColumn fx:id="colCity" prefWidth="83.0" text="Город" />
            <TableColumn fx:id="colStadiumName" prefWidth="83.0" text="Название" />
            <TableColumn fx:id="colCapacity" prefWidth="90.0" text="Вместимость" />
            <TableColumn fx:id="colTicketPrice" prefWidth="90.0" text="Цена билета" />
         </columns>
      </TableView>
      <Button fx:id="btnInsert" layoutX="117.0" layoutY="219.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="80.0" text="добавить" />
      <Button fx:id="btnDelete" layoutX="209.0" layoutY="219.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="80.0" text="удалить" />
      <Button fx:id="btnSelectCity" layoutX="335.0" layoutY="27.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="25.0" prefWidth="80.0" text="Выбрать" />
   </children>
</AnchorPane>
